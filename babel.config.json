{
	"presets": [
		[
			"@babel/preset-env",
			{
				"corejs": 3.16,
				"useBuiltIns": "usage"
			}
		],
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["."],
				"alias": {}
			}
		],
		[
			"babel-plugin-styled-components",
			{
				"transpileTemplateLiterals": true
			}
		]
	]
}
